{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "gh-pages": "^3.1.0",
    "gsap": "^3.6.0",
    "js-cookie": "^3.0.1",
    "markdown-to-jsx": "^7.1.7",
    "react": "^17.0.1",
    "react-countup": "^6.1.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.1",
    "react-intersection-observer": "^8.33.1",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "three": "^0.126.1",
    "vanta": "^0.5.21",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretty": "prettier --write \"./**/*.{ts,tsx,css,js,jsx,json}\" && git add -u",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "husky-install": "husky install",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write \"./**/*.{ts,tsx,css,js,jsx,json}\"",
      "eslint --cache --fix"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write \"./**/*.{ts,tsx,css,js,jsx,json}\""
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "prettier-format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
  "homepage": "https://aandrys.github.io/Portfolio2"
}
